RewriteEngine On    # Turn on the rewriting engine
RewriteRule ^login$ login.php [NC,L]
RewriteRule ^index$ index.php [NC,L]
RewriteRule ^read/([0-9_-]*)$ read.php?id=$1 [NC,L]
RewriteRule ^menupages$ menupages.php [NC,L]
RewriteRule ^menupages/([0-9_-]*)$ menupages.php?category=$1&page=$1 [NC,L]
RewriteRule ^topmenuitems$ topmenuitems.php [NC,L]
RewriteRule ^topmenuitems/([0-9_-]*)$ topmenuitems.php?page=$1 [NC,L]
RewriteRule ^logout$ logout.php [NC,L]
RewriteRule ^session$ session.php [NC,L]
RewriteRule ^delete$ delete.php [NC,L]